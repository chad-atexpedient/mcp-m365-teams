# Microsoft 365 / Azure AD Configuration
# Copy this file to .env and fill in your values

# Your Azure AD Tenant ID
M365_TENANT_ID=your-tenant-id-here

# Your Azure AD Application (Client) ID
M365_CLIENT_ID=your-client-id-here

# Your Azure AD Application Client Secret
M365_CLIENT_SECRET=your-client-secret-here

# Optional: Set log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
